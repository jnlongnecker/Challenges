<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Contact_age_minimum_10</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(ISBLANK( Birthdate )) &amp;&amp; YEAR(TODAY()) - YEAR(Birthdate) &lt; 10</errorConditionFormula>
    <errorDisplayField>Birthdate</errorDisplayField>
    <errorMessage>Students must be at least 10 years old.</errorMessage>
</ValidationRule>
